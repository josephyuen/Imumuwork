<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zumumu.imumu">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"></uses-permission>
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE"></uses-permission>
    <uses-permission android:name="android.permission.READ_PHONE_STATE"></uses-permission>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"></uses-permission>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"></uses-permission>

    <application
        android:name="com.nzf.mvpframe.baseapp.BaseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/imumu_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ui.splash.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.user.activity.LoginActivity"></activity>
        <activity android:name=".ui.user.activity.SigninActivity"></activity>
        <activity android:name=".ui.user.activity.ForgetActivity"></activity>
        <activity android:name=".ui.home.HomeActivity"></activity>
        <activity android:name=".ui.shop.activity.ProductDetailActivity"></activity>
        <activity android:name=".ui.shop.activity.ProductCommentActivity"></activity>
        <activity android:name=".ui.shop.activity.ShopListActivity"></activity>
        <activity android:name=".ui.user.activity.PersonInfoActivity"></activity>
        <activity android:name=".ui.shop.activity.MyIndentActivity"></activity>
        <activity android:name=".ui.personcenter.activity.MyAddressActivity"></activity>
        <activity android:name=".ui.shop.activity.ShopCartActivity"></activity>
        <activity android:name=".ui.pay.activity.AccountsActivity"></activity>
        <activity android:name=".ui.personcenter.activity.AddAddressActivity"></activity>
        <activity android:name=".ui.personcenter.activity.ModifyMyAddressActivity"></activity>
        <activity android:name=".ui.pay.activity.OrderAddressActivity"></activity>
        <activity android:name=".ui.pay.activity.PayActivity"></activity>

        <activity
            android:name="com.alipay.sdk.app.H5PayActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>
        <activity
            android:name="com.alipay.sdk.auth.AuthActivity"
            android:configChanges="orientation|keyboardHidden|navigation"
            android:exported="false"
            android:screenOrientation="behind" >
        </activity>



    </application>

</manifest>